{
  "compilerOptions": {
    "target": "es2021",
    "module": "es6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "alwaysStrict": true,
    "baseUrl": ".",
    "moduleResolution": "Node",
    "paths": {
      "@minecraft/*": ["./node_modules/@minecraft/*"]
    },
    "outDir": "scripts"
  }
}